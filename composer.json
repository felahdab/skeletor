{
    "name": "fanlab/skeletor",
    "type": "project",
    "description": "Projet Skeletor: framework applicatif pour les besoins organiques de la FAN",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "althinect/filament-spatie-roles-permissions": "^2.2",
        "asantibanez/livewire-charts": "^3.0",
        "axonc/filament-copyable-placeholder": "^0.3.0",
        "backpack/basset": "^1.2",
        "barryvdh/laravel-debugbar": "^3.7",
        "calebporzio/sushi": "^2.4",
        "coolsam/modules": "^3",
        "darkaonline/l5-swagger": "^8.5",
        "filament/filament": "^3.1",
        "filament/spatie-laravel-google-fonts-plugin": "^3.1",
        "glorand/laravel-model-settings": "^7.0",
        "guzzlehttp/guzzle": "^7.2",
        "hisorange/browser-detect": "^5.0",
        "ijpatricio/mingle": "^0.0.14",
        "joshbrw/laravel-module-installer": "^2.0",
        "lab404/laravel-impersonate": "^1.7",
        "laravel-shift/blueprint": "^2.7",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.0",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^3.0",
        "mhmiton/laravel-modules-livewire": "^3.0",
        "nunomaduro/collision": "^7.0",
        "nwidart/laravel-modules": "^11.0",
        "owen-it/laravel-auditing": "^13.5",
        "predis/predis": "^2.2",
        "rappasoft/laravel-livewire-tables": "^3.2",
        "saade/filament-fullcalendar": "^3.0",
        "socialiteproviders/keycloak": "^5.2",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-data": "^4.0",
        "spatie/laravel-html": "^3.2",
        "spatie/laravel-markdown": "^2.2",
        "spatie/laravel-permission": "^6.0",
        "stancl/tenancy": "^3.7",
        "tomsix/laravel-components-library": "^2.0",
        "twbs/bootstrap-icons": "^1.10",
        "wnx/laravel-backup-restore": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel-shift/blueprint": "^2.7",
        "laravel/dusk": "^8.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "livewire/livewire"
            ]
        },
        "merge-plugin": {
            "include": [
                "Modules/*/composer.json"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "joshbrw/laravel-module-installer": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
